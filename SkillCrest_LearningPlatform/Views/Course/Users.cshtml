@model IEnumerable<UserShortDetailsViewModel> 

@{
    ViewData["Title"] = "User List";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                
            </tr>
        }
    </tbody>
</table>